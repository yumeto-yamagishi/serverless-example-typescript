{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "todo-list app",
  "main": "handler.js",
  "scripts": {
    "test:rest-api": "httpyac check/rest-api.http --env dev --all",
    "test:rest-api:validation": "httpyac check/rest-api-validation.http --env dev --all",
    "deploy:prod": "sls deploy --stage prod",
    "package:prod": "sls package --stage prod",
    "undeploy:prod": "sls remove --stage prod",
    "start:offline": "sls offline start --stage dev",
    "start:offline:debug": "SLS_DEBUG=1 sls offline start --stage dev",
    "dynamodb:install": "sls dynamodb install",
    "lint": "eslint --fix",
    "prettier": "prettier -w src",
    "generate:refs:sls": "sls create -t aws-nodejs-typescript -p ref_apps/sls",
    "pregenerate:refs:sls": "mkdir -p ref_apps & rm -rf ref_apps/sls"
  },
  "dependencies": {
    "@middy/core": "^3.1.1",
    "@middy/http-cors": "^3.1.1",
    "@middy/http-error-handler": "^3.1.1",
    "@middy/http-event-normalizer": "^3.1.1",
    "@middy/http-json-body-parser": "^3.1.1",
    "@middy/validator": "^3.1.1",
    "aws-sdk": "^2.1190.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/node": "^10.17.60",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "esbuild": "^0.14.54",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.1",
    "httpyac": "^5.6.1",
    "json-schema-to-ts": "^2.5.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "serverless": "^3.21.0",
    "serverless-api-gateway-throttling": "^1.2.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.32.5",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-plugin-subscription-filter": "^1.0.7",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "author": "noboru-kudo",
  "license": "MIT"
}
