{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "todo-list app",
  "main": "handler.js",
  "scripts": {
    "test:rest-api": "httpyac check/rest-api.http --env dev --all",
    "test:rest-api:validation": "httpyac check/rest-api-validation.http --env dev --all",
    "deploy:prod": "sls deploy --stage prod",
    "package:prod": "sls package --stage prod",
    "undeploy:prod": "sls remove --stage prod",
    "start:offline": "sls offline start --stage dev",
    "start:offline:debug": "SLS_DEBUG=1 sls offline start --stage dev",
    "dynamodb:install": "sls dynamodb install",
    "lint": "eslint --fix",
    "prettier": "prettier -w src",
    "generate:refs:sls": "sls create -t aws-nodejs-typescript -p ref_apps/sls",
    "pregenerate:refs:sls": "mkdir -p ref_apps & rm -rf ref_apps/sls"
  },
  "dependencies": {
    "@middy/core": "^3.1.0",
    "@middy/http-cors": "^3.1.0",
    "@middy/http-error-handler": "^3.1.0",
    "@middy/http-event-normalizer": "^3.1.0",
    "@middy/http-json-body-parser": "^3.1.0",
    "@middy/validator": "^3.1.0",
    "aws-sdk": "^2.848.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.17.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "esbuild": "^0.14.39",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "httpyac": "^5.5.1",
    "json-schema-to-ts": "^2.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "serverless": "^3.15.2",
    "serverless-api-gateway-throttling": "^1.1.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-esbuild": "^1.27.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-aws-alerts": "^1.7.1",
    "serverless-plugin-subscription-filter": "^1.0.7",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.5"
  },
  "author": "noboru-kudo",
  "license": "MIT"
}
